<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

    <record id="as_planilla_general_views" model="ir.ui.view">
        <field name="name">Planilla General</field>
        <field name="model">as.planilla.general</field>
        <field name="arch" type="xml">
            <form string="Planilla General ">
                <group col="4">
                    <!-- <field name="as_sale"/> -->
                    <field name="as_partner_id"/>
                 
                    
                </group>
                <footer>
                    <button name="export_xls" 
                        default_focus="1"
                        string="Imprimir" 
                        type="object" 
                        class="oe_highlight"
                        context="{'xls_export':1}" 
                        icon="fa-download"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_planilla_report" model="ir.actions.act_window">
        <field name="name">Planilla General </field>
        <field name="res_model">as.planilla.general</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="as_planilla_general_views"/>
        <field name="target">new</field>
    </record>

  <menuitem id="menu_planilla_general" name="Planilla General" action="action_planilla_report" parent="account_reports.account_reports_partners_reports_menu" sequence="21" />
</data>
</odoo>
