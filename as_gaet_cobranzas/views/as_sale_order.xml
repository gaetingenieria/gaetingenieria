<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- Sale Order form view -->
	<record id="view_order_form_inherit_gaet_view" model="ir.ui.view">
		<field name="name">view.order.form.inherit.view</field>
		<field name="model">sale.order</field>
		<field name="inherit_id" ref="sale.view_order_form" />
		<field name="priority">55</field>
		<field name="arch" type="xml">

			<xpath expr="//field[@name='partner_id']" position="before">
				<field name="as_numeracion_interna" />
			</xpath>	
			<xpath expr="//field[@name='partner_id']" position="after">
				<field name="as_type_retencion" />
			</xpath>
			<xpath expr="//field[@name='date_order']" position="after">
				<field name="as_retencion" />
				<field name="as_date_fiscal" />
				<field name="as_cont_invoice" readonly="1"/>
				<field name="as_medio_pago" />
				<field name="as_tipo_garantia" />
			</xpath>

		</field>
	</record>
	<record id="as_sale_view_sale_order_form" model="ir.ui.view">
		<field name="name">as.sale.order.form</field>
		<field name="model">sale.order</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="sale.view_order_tree" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='name']" position="before">
				<field name="as_numeracion_interna" readonly="1"/>
			</xpath>

		</field>
	</record>
</odoo>