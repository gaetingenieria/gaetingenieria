<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_comercial" model="ir.ui.view">
        <field name="name">res.company.form.inherit.comercial</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='favicon']" position="after">
                <field name="representante_name" />
                <field name="representante_rut" />
                <field name="comuna" />
                <field name="icon_image" />
            </xpath>
        </field>
    </record>

</odoo>
