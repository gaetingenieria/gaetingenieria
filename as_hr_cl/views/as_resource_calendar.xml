<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="as_cl_hr_salary_rule_view" model="ir.ui.view">
            <field name="name">payroll_resource_calendar_view_form.rule.view</field>
            <field name="model">resource.calendar</field>
            <field name="inherit_id" ref="hr_payroll.payroll_resource_calendar_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='hours_per_day']" position="after">
                    <field name="as_total_days" />
                    <field name="as_tiempo_tolerancia" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>